    "piping": "^0.3.0",
    "pretty-error": "^1.2.0",
    "query-string": "^3.0.0",
    "react": "^0.14.2",
    "react-dom": "^0.14.1",
    "react-helmet": "^2.2.0",
    "react-redux": "^4.0.0",
    "react-router": "1.0.3",
